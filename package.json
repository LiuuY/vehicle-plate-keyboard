{
  "name": "vehicle-plate-keyboard",
  "license": "MIT",
  "version": "0.8.4",
  "description": "Chinese vehicle plate number keyboard for React",
  "author": "LiuuY <mail4c59@gmail.com>",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "webpack",
    "build:watch": "webpack --watch --info-verbosity verbose",
    "build": "rimraf dist && NODE_ENV=production webpack",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prestart": "rimraf dist && tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@storybook/addon-actions": "^6.0.0",
    "@storybook/addon-knobs": "^6.0.0",
    "@storybook/addon-links": "^6.0.0",
    "@storybook/addon-viewport": "^6.0.0",
    "@storybook/addons": "^6.0.0",
    "@storybook/react": "^6.0.0",
    "@svgr/webpack": "^4.1.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-px-to-viewport": "^0.0.3",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.14.2",
    "react": "^17.0.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^17.0.1",
    "sass": "^1.28.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "yarn lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
